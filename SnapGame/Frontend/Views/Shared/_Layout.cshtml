<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Frontend</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <script type="text/javascript" src="~/lib/axios.min.js"></script>
    <link rel="stylesheet" href="~/css/site.css"/>
</head>
<body style="background-image: url(https://media.istockphoto.com/photos/playing-cards-fall-on-a-black-table-picture-id1187186401?k=6&m=1187186401&s=612x612&w=0&h=S6qxSZgd5mTlmGeZAxc-C3VQ2CBvnIkwjZkm4LcwV3Q=);
        height: 100vh;
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center;">
       
        
    <div class="cover-container d-flex h-100 flex-column p-3 mx-auto">
        <main role="main" class="inner cover h-100">
            @RenderBody()
        </main>
    </div>

   
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
